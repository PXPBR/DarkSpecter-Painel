# Fun√ß√£o para mostrar o menu principal
def mostrar_menu():
    print("1 - PUXAR CEP")
    print("2 - PUXAR IP")
    print("3 - PUXAR BIN")
    print("4 - PUXAR DDD")
    print("5 - PUXAR CNPJ")
